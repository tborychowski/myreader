<p>Via <a href="http://onlinelibrary.wiley.com/doi/10.1111/j.1474-919X.1888.tb07738.x/abstract?systemMessage=Wiley+Online+Library+will+be+disrupted+on+11+May+from+10%3A00-12%3A00+BST+%2805%3A00-07%3A00+EDT%29+for+essential+maintenance" target="_blank">Ibis</a>, <a href="http://www.jstor.org/stable/1536392" target="_blank">The Biological Bulletin</a>, <a href="http://www.birdresearch.dk/unilang/raritet/peafowl/peafowl.htm" target="_blank">Bird Research</a>, <a href="http://www.feathersite.com/Poultry/BRKChange.html" target="_blank">Feather Site</a>.</p>";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:8:"category";a:5:{i:0;a:5:{s:4:"data";s:7:"biology";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:0:"";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:1;a:5:{s:4:"data";s:7:"science";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:0:"";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:2;a:5:{s:4:"data";s:6:"gender";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:0:"";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:3;a:5:{s:4:"data";s:8:"estrogen";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:0:"";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}i:4;a:5:{s:4:"data";s:15:"science history";s:7:"attribs";a:1:{s:0:"";a:1:{s:6:"domain";s:0:"";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:7:"pubDate";a:1:{i:0;a:5:{s:4:"data";s:28:"Tue, 7 May 2013 14:20:00 GMT";s:7:"attribs";a:0:{}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}s:4:"guid";a:1:{i:0;a:5:{s:4:"data";s:9:"493154825";s:7:"attribs";a:1:{s:0:"";a:1:{s:11:"isPermaLink";s:5:"false";}}s:8:"xml_base";s:0:"";s:17:"xml_base_explicit";b:0;s:8:"xml_lang";s:0:"";}}}s:32:"http://purl.org/dc/elements/1.1/";a:1:{s:7:"creator";a:1:{i:0;a:5:{s:4:"data";s:20:"Esther Inglis-Arkell";s:7:"attribs";a:0:{}s:8:"xml_base";s